{
    "name": "playwright-api",
    "version": "1.0.0",
    "description": "API REST profissional com Playwright para automação web",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "node test.js",
        "docker:build": "docker build -t playwright-api .",
        "docker:run": "docker run -p 3000:3000 -e API_TOKEN=e6b7bba2-7d51-46a4-8645-22be9483cb18",
        "docker:compose": "docker-compose up -d",
        "docker:logs": "docker-compose logs -f"
    },
    "keywords": [
        "playwright",
        "automation",
        "web-scraping",
        "testing",
        "api"
    ],
    "author": "Bovichain",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.4.1",
        "helmet": "^7.2.0",
        "morgan": "^1.10.1",
        "@supabase/supabase-js": "^2.45.0",
        "playwright": "1.56.1"
    },
    "devDependencies": {
        "nodemon": "^3.1.7"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
